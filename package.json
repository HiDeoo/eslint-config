{
  "name": "@hideoo/eslint-config-workspace",
  "description": "HiDeoo's ESLint configuration",
  "homepage": "https://github.com/HiDeoo/eslint-config",
  "version": "0.3.0",
  "author": "HiDeoo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "prettier -c . && eslint . --max-warnings=0",
    "release": "pnpm -r publish --access public",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@hideoo/eslint-config": "workspace:*",
    "@hideoo/prettier-config": "0.0.1",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "base",
    "react",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/eslint-config"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown --cache"
  },
  "packageManager": "pnpm@7.4.0",
  "prettier": "@hideoo/prettier-config"
}
